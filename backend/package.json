{
  "name": "more-app-backend",
  "version": "0.0.1",
  "description": "MORE App (モア アプリ) backend service",
  "private": true,
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "prisma:generate": "prisma generate",
    "prisma:migrate:diff": "DATABASE_URL=$DATABASE_URL prisma migrate diff --from-url \"$DATABASE_URL\" --to-schema-datamodel prisma/schema.prisma --script",
    "prisma:migrate:deploy": "DATABASE_URL=$DATABASE_URL prisma migrate deploy",
    "prisma:migrate:verify": "node scripts/verify-no-shadow.js",
    "seed": "ts-node prisma/seed.ts",
    "prisma:seed": "ts-node prisma/seed.ts",
    "seed:community-tags": "ts-node prisma/seed-community-tags.ts",
    "postinstall": "npm run prisma:generate",
    "verify:payments-replay": "ts-node scripts/verify-payments-replay.ts",
    "verify:payments-out-of-order": "ts-node scripts/verify-payments-out-of-order.ts",
    "verify:payments-partial-refund": "ts-node scripts/verify-payments-partial-refund.ts",
    "test:payout-policy": "node --test -r ts-node/register tests/organizer-payout-policy.test.ts"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^5.13.0",
    "@google-cloud/vision": "^5.3.4",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.0",
    "@prisma/client": "^6.19.0",
    "@google-cloud/storage": "^7.13.0",
    "axios": "^1.13.2",
    "bullmq": "^5.26.3",
    "build": "^0.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "openai": "^6.9.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "stripe": "^17.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.12.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "prisma": "^6.19.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
